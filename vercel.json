{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "BOOKINGS_DATABASE": "@bookings-database",
        "GOOGLEPLACES_API_KEY": "@googleplaces-api-key"
    }
}